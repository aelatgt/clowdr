{
    "name": "clowdr-playout",
    "version": "1.0.0",
    "description": "Video playout service",
    "main": "build/server.js",
    "scripts": {
        "build": "tsc",
        "start": "node build/server.js",
        "dev": "nodenv -E ./.env -e \"tsc-watch --onSuccess \\\"npm run-script --prefix ../../ run-services-playout\\\"\"",
        "graphql-codegen": "graphql-codegen --config codegen.js"
